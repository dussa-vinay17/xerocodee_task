{
    "presets": ["next/babel"],
    "plugins": [
      [
        "babel-plugin-transform-imports",
        {
          "@mui/material": {
            "transform": "@mui/material/esm/${member}",
            "preventFullImport": true
          },
          "@mui/icons-material": {
            "transform": "@mui/icons-material/esm/${member}",
            "preventFullImport": true
          }
        }
      ],
      "babel-plugin-transform-async-to-promises"
    ]
  }
  